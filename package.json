{
  "name": "webpack-boilerplate",
  "version": "1.1.0",
  "description": "Webpack boilerplate with babel and sass",
  "author": "",
  "main": "app.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.config.js --mode production",
    "eslint": "eslint 'src/**/*.js' 'src/spec/**/*.spec.js'",
    "serve": "NODE_ENV=development webpack-dev-server --mode development",
    "stryker": "stryker run",
    "test": "jest --config jest.conf.js --detectOpenHandles"
  },
  "keywords": [
    "webpack",
    "babel",
    "sass"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@stryker-mutator/core": "^4.5.1",
    "@stryker-mutator/jest-runner": "^4.5.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.24.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "bootstrap": "^4.1.3"
  }
}
